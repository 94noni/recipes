                "config/packages/lock.yaml": {
                    "contents": "framework:\n    lock: '%env(LOCK_DSN)%'\n",
                    "executable": false,
                    "encoding": ""
                }
            },
            "origin": "symfony/lock:4.4@github.com/symfony/recipes:master",
            "is_contrib": false
        }
